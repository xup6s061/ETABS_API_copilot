<Window x:Class="YourNamespace.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:YourNamespace.ViewModels"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <vm:MainViewModel />
    </Window.DataContext>
    <Grid>
        <Button Name="OpenETABSButton" Content="Open ETABS" HorizontalAlignment="Center" VerticalAlignment="Center" Command="{Binding OpenETABSCommand}"/>
        <Button Name="CreateNewETABSFileButton" Content="Create New ETABS File" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,50,0,0" Command="{Binding CreateNewETABSFileCommand}"/>
    </Grid>
</Window>
